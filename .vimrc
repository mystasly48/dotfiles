" encoding
set fenc=utf-8

" backup
set nobackup

" swapfile
set noswapfile

" autoreload
set autoreload

" show typing command
set showcmd

" show number
set number

" cursor current line
set cursorline

" smart indent
set smartindent

set showmatch

set expandtab

set tabstop=2
set shiftwidth=2

set wrapscan

